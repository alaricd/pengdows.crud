        <PackageReference Include="Npgsql" Version="9.0.3" />
        <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="23.8.0" />
        <PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2"/>
